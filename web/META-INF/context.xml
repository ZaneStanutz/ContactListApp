<?xml version="1.0" encoding="UTF-8"?>
<Context path="/contactList">
    
    <Resource name="jdbc/contact" auth="Container"
        driverClassName="com.mysql.jdbc.Driver"
        url="jdbc:mysql://localhost:3306/contact"
        username="contact_user" password="root"
        maxActive="100" maxIdle="30" maxWait="1000"
        removeAbandonTimeout="60" type="javax.sql.DataSource"/>
    
</Context>
